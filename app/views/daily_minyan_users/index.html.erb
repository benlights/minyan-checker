<p id="notice"><%= notice %></p>

<h1>Daily Minyan Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_minyan_users.each do |daily_minyan_user| %>
      <tr>
        <td><%= link_to 'Show', daily_minyan_user %></td>
        <td><%= link_to 'Edit', edit_daily_minyan_user_path(daily_minyan_user) %></td>
        <td><%= link_to 'Destroy', daily_minyan_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily Minyan User', new_daily_minyan_user_path %>
